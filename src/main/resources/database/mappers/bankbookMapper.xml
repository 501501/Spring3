<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.sol.s1.bankbook.BankBookDAO">
	<insert id="setInsert" parameterType="com.sol.s1.bankbook.BankBookDTO">
		INSERT INTO BANKBOOK (bookNumber, bookName, bookRate, bookSale) 
		VALUES (bankbook_sqe.nextval, #{bookName}, #{bookRate}, #{bookSale})
	</insert>
	
	<select id="getList" resultType="com.sol.s1.bankbook.BankBookDTO">
		SELECT * FROM BANKBOOK
	</select>
	
	<select id="getSelect" parameterType="com.sol.s1.bankbook.BankBookDTO" resultType="com.sol.s1.bankbook.BankBookDTO">
		SELECT * FROM BANKBOOK WHERE BOOKNUMBER = #{bookNumber}
	</select>
 </mapper>